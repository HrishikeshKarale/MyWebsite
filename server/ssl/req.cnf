# https://www.phcomp.co.uk/Tutorials/Web-Technologies/Understanding-and-generating-OpenSSL.cnf-files.html
[req]
default_bits		= 2048
distinguished_name = req_distinguished_name
x509_extensions = v3_req
prompt = no
[req_distinguished_name]
C = US
ST = New York
L = Rochester
O = Kkiokio
OU = Web Server
CN = localhost:8001, 10.0.1.3:8001
[v3_req]
keyUsage = digitalSignature, keyAgreement
extendedKeyUsage = serverAuth
subjectAltName = @alt_names
[alt_names]
DNS.1 = www.localhost:8001
DNS.2 = https://localhost:8001
DNS.3 = localhost:8001
DNS.4 = www.10.0.1.3:8001
DNS.5 = https://10.0.1.3:8001
DNS.6 = 10.0.1.3:8001

# https://stackoverflow.com/questions/21397809/create-a-trusted-self-signed-ssl-cert-for-localhost-for-use-with-express-node
# C:\"Program Files"\Git\usr\bin\openssl.exe  req -x509 -nodes -days 365 -newkey rsa:2048 -keyout cert.key -out cert.pem -config req.cnf -sha256